  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "vitest": "vitest",
    "e2e": "exec playwright test",
    "e2e:debug": "exec playwright test --ui",
    "e2e:gen": "npx playwright codegen --load-storage=e2e/auth.json http://localhost:3000",
    "lint": "next lint",
    "prepare": "husky",
    "sanity": "echo 'Sanity check'",
    "s:gentypes": "npx supabase gen types --lang typescript --project-id tnoiejxfeosoxlfiptws > ./lib/supabase/database.types.ts",
    "sb:proxy": "local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "sb:login": "storyblok login",
    "sb:pull-components": "storyblok pull-components --space 154372 --path ./lib/storyblok/",
    "sb:push-components": "storyblok push-components ./lib/storyblok/components.154372.json --space 154372 --presets-source ./lib/storyblok/presets.154372.json",
    "sb:generate-types": "storyblok generate-typescript-typedefs --sourceFilePaths ./lib/storyblok/components.154372.json --destinationFilePath ./lib/storyblok/component-types-sb.d.ts",
    "sb:pull-types": "pnpm sb:pull-components && pnpm sb:generate-types",
    "plop": "plop  --plopfile ./generators/plopfile.mjs"
  },
